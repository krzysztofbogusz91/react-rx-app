{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "import/no-duplicates": 0,
    "react/jsx-one-expression-per-line": 0,
    "import/no-named-as-default": 0,
    "react/no-unused-prop-types": 0,
    "react/jsx-filename-extension": [
      2, { "extensions": [".js"] }
    ]
  }
}
